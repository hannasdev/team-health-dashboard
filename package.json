{
  "name": "team-health-dashboard",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/app.js",
    "build": "tsc -p tsconfig.prod.json",
    "dev": "ts-node-dev --respawn --transpile-only --experimental-specifier-resolution=node src/app.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "express": "^4.18.2",
    "googleapis": "^118.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
